{
  "name": "react-template",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.6.3",
    "@auth0/auth0-react": "^1.6.0",
    "@chargebee/chargebee-js-react-wrapper": "^0.4.3",
    "@craco/craco": "^6.0.0",
    "@emotion/css": "^11.0.0",
    "@emotion/react": "^11.1.2",
    "@emotion/styled": "^11.0.0",
    "@splitsoftware/splitio-react": "^1.3.0",
    "antd": "^4.16.9",
    "craco-less": "^1.18.0",
    "date-fns": "^2.25.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/prompt": "^11.0.0",
    "@ls-age/commitlint-circle": "^1.0.0",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.10",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/jest": "^26.0.24",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "commitizen": "^4.2.2",
    "cypress": "^8.5.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.1",
    "file-loader": "^6.2.0",
    "git-cz": "^4.7.6",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "prettier": "^2.2.1",
    "react-scripts": "4.0.1",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^2.0.1",
    "typescript": "~4.1.2"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -s build:css build:js",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "build:js": "craco build",
    "commit": "git-cz",
    "commit:retry": "npm run commit -- --retry",
    "eject": "react-scripts eject",
    "cy:run": "cypress run",
    "integration": "NODE_ENV=test npm run start & npx cypress open",
    "integration:headless": "NODE_ENV=test start-server-and-test start http://localhost:3000 cy:run",
    "integration:headless:https": "HTTPS=true NODE_ENV=test START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start https-get://localhost:3000 cy:run",
    "lint": "eslint . --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "start": "REACT_APP_COMMIT_REF=$(git rev-parse --short HEAD) REACT_APP_AUTH0_DOMAIN=$(grep REACT_APP_AUTH0_DOMAIN .env | cut -d '=' -f2) REACT_APP_AUTH0_CLIENT_ID=$(grep REACT_APP_AUTH0_CLIENT_ID .env | cut -d '=' -f2)  npm-run-all -p start:*",
    "start:css": "postcss src/tailwind.css -o src/index.css --watch",
    "start:js": "sleep 2 && craco start",
    "storybook": "start-storybook -p 9009 -s public",
    "storybook:build": "npm run build:css && build-storybook -s public",
    "test": "craco test"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/@commitlint/prompt"
    }
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "jest": true,
      "cypress/globals": true
    },
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "plugin:prettier/recommended",
      "plugin:import/typescript",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/react",
      "plugin:testing-library/react"
    ],
    "parserOptions": {
      "project": "tsconfig.json",
      "tsconfigRootDir": "."
    },
    "plugins": [
      "import",
      "react",
      "@typescript-eslint",
      "react-hooks",
      "testing-library"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": [
        "warn",
        {
          "allowExpressions": true,
          "allowTypedFunctionExpressions": true
        }
      ],
      "@typescript-eslint/explicit-member-accessibility": 0,
      "@typescript-eslint/no-use-before-define": 0,
      "array-callback-return": "warn",
      "import/no-default-export": "error",
      "import/no-unresolved": 2,
      "import/order": [
        "error",
        {
          "alphabetize": {
            "order": "asc"
          }
        }
      ],
      "jest/no-hooks": 0,
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ],
      "react/no-unescaped-entities": [
        "error",
        {
          "forbid": [
            ">",
            "}",
            "\""
          ]
        }
      ],
      "react/prop-types": "off",
      "react/self-closing-comp": "error",
      "react-hooks/exhaustive-deps": "error",
      "react-hooks/rules-of-hooks": "error"
    },
    "overrides": [
      {
        "files": [
          "./**/*.js"
        ],
        "rules": {
          "@typescript-eslint/no-var-requires": "off"
        }
      },
      {
        "files": [
          "./**/*.stories.tsx"
        ],
        "rules": {
          "import/no-default-export": "off",
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      },
      {
        "files": [
          "*.test.ts",
          "*.test.tsx"
        ],
        "extends": [
          "plugin:jest/all"
        ],
        "rules": {
          "jest/prefer-expect-assertions": "off"
        }
      },
      {
        "files": [
          "*.spec.ts",
          "*.spec.tsx"
        ],
        "plugins": [
          "cypress"
        ],
        "extends": "plugin:cypress/recommended"
      }
    ]
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
